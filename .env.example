# Supabase Local Development (Next.js)
# これらの値は `supabase status` で確認できます

# Supabase API URL（公開用）
NEXT_PUBLIC_SUPABASE_URL=http://127.0.0.1:54321

# Supabase Anon Key（公開用・クライアント側で使用）
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Supabase Service Role Key（サーバー側専用・機密情報）
# ⚠️ クライアント側では絶対に使用しないでください
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Database URL（Prisma用）
DATABASE_URL=postgresql://postgres:postgres@127.0.0.1:54322/postgres

# Direct URL（Prisma Migrate用）
DIRECT_URL=postgresql://postgres:postgres@127.0.0.1:54322/postgres

# Google Maps API Key（公開用）
# ⚠️ Google Cloud Console で Maps JavaScript API を有効化し、APIキーを取得してください
# https://console.cloud.google.com/apis/credentials
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
